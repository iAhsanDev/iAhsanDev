<mvc:View
	controllerName="sap.ui.demo.nav.controller.employee.TransferOC"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form"
	busyIndicatorDelay="0">
	<Page
		id="employeePage"
		title="{i18n>TrasferOCTitle}"
		showNavButton="true"
		navButtonPress=".onNavBack"
		class="sapUiResponsiveContentPadding">
		<content>


		<Panel headerText="Details"
		class="sapUiMediumMarginBottom">
			<List>
			<InputListItem label="Pallet Number " class="sapUiMediumMarginBottom">
				<Input
				 id="PalletNinput"	 value="{item}" placeholder="Scan Pallet Number"/>
				</InputListItem>

			</List> 
		</Panel>
	

<VBox>
	
			<FlexBox
				alignItems="Start"
				justifyContent="Center">
				<Button id="confirm" text="{i18n>btncon}" press=".onConfirm" class="sapUiTinyMarginEnd"/>
       			 <Button id="clearall" text="{i18n>btnclear}" press=".onClearall" class="sapUiTinyMarginEnd"/> 
			</FlexBox>
	

</VBox>
	




		</content>
	</Page>
</mvc:View>